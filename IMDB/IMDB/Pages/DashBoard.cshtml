@page
@model WebApplication2.Pages.DashBoard

@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <link rel="stylesheet" href="/css/dash.css">
    <title>Dashboard</title>
</head>
<body>
<div>
    <table class="styled-table">
        <thead>
        <tr>
            <th>Name</th>
            <th>Birth year</th>
            <th>Death year</th>
            <th>Primary profession</th>
            <th>Known for titles</th>
        </tr>
        </thead>
        <tbody>
        @for (int i = 0; i < 10; i++)
        {
            <tr>
                <td>@i</td>
                <td>@i</td>
                <td>@i</td>
                <td>@i</td>
                <td>@i</td>
            </tr>
        }
        </tbody>
    </table>
</div>
</body>
</html>