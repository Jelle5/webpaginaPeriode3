@page
@using WebApplication2.Pages.Database.model
@model WebApplication2.Pages.Test

@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <!--<link rel="stylesheet" href="/css/dash.css">-->
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@4.3.1/dist/css/bootstrap.min.css" integrity="sha384-ggOyR0iXCbMQv3Xipma34MD+dH/1fQ784/j6cY/iJTQUOhcWr7x9JvoRxT2MZw1T" crossorigin="anonymous">
    <title>Dashboard</title>
</head>
<body>
<div>
    <h1>@Model.Tconsts</h1>
    <a href="https://imdb.com/title/@Model.Tconsts">LInk</a>
</div>
<div>
    <table class="table table-hover">
        <thead>
        <tr>
            <th>PrimaryName</th>
            <th>birthYear</th>
            <th>deathYear</th>

        </tr>
        </thead>
        <tbody>
        @foreach (var individual in Model.individuals)
        {
            <tr>
                <td><a href="https://imdb.com/name/@individual.nconst">@individual.primary</a></td>
                <td>@individual.birthyear</td>
                <td>@individual.deathyear</td>

            </tr>
        }
        </tbody>
    </table>
</div>
</body>
</html>